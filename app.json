{
  "expo": {
    "name": "Musti Place",
    "slug": "food-delivery-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FF6B6B"
    },
    "description": "Food delivery app - Order delicious meals from your favorite restaurants",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mustiplace.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      },
      "runtimeVersion": {
        "policy": "appVersion"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FF6B6B"
      },
      "package": "com.mustiplace.app",
      "versionCode": 1,
      "runtimeVersion": "1.0.0",
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "INTERNET",
        "ACCESS_NETWORK_STATE",
        "RECEIVE_BOOT_COMPLETED",
        "VIBRATE"
      ],
      "blockedPermissions": [],
      "playStoreUrl": "https://play.google.com/store/apps/details?id=com.mustiplace.app",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "*.mustiplace.com"
            },
            {
              "scheme": "mustiplace"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router"
    ],
    "scheme": "mustiplace",
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "cde5bd2d-b03a-427c-91ee-b0e021e2dc17"
      }
    },
    "owner": "millionin20s",
    "updates": {
      "url": "https://u.expo.dev/cde5bd2d-b03a-427c-91ee-b0e021e2dc17"
    }
  }
}
