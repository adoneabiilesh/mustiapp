{
  "name": "musti-place",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "expo lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --coverage --watchAll=false --ci",
    "test:webhooks": "ts-node scripts/test-webhooks.ts",
    "validate:env": "ts-node scripts/validate-env.ts",
    "security:audit": "ts-node -e \"import('./lib/security/audit').then(m => m.runSecurityAudit().then(r => console.log(JSON.stringify(r, null, 2))))\"",
    "type-check": "tsc --noEmit",
    "pre-deploy": "npm run validate:env && npm run test:ci && npm run security:audit",
    "supabase:schema": "echo Run scripts/create-supabase-schema.sql in Supabase SQL editor",
    "supabase:seed": "node scripts/seed-menu-from-images.js",
    "supabase:test": "node scripts/test-connection.js",
    "setup:check": "node scripts/check-setup.js",
    "setup:db": "node scripts/setup-database.js",
    "admin:dev": "cd admin-dashboard && npm run dev",
    "admin:build": "cd admin-dashboard && npm run build",
    "admin:install": "cd admin-dashboard && npm install"
  },
